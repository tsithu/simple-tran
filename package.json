{
  "name": "simple-tran",
  "version": "0.0.1",
  "description": "Simple Transaction",
  "keywords": [
    "Vue",
    "Koa",
    "Apollo",
    "Nuxt",
    "Objection"
  ],
  "author": {
    "name": "Toe Si Thu",
    "email": "tsithu@gmail.com"
  },
  "contributors": [],
  "private": false,
  "license": "MIT",
  "scripts": {
    "cc": "yarn cache clean",
    "i": "yarn install",
    "raw": "cross-env NODE_ENV=development node ./server/index.js",
    "serve": "yarn dev",
    "serve:prod": "yarn prod",
    "prod": "yarn build && yarn start",
    "dev": "cross-env NODE_ENV=development backpack dev",
    "test": "yarn lint && yarn test:server && yarn test:client",
    "build": "nuxt build && backpack build",
    "bstart": "backpack build && yarn start",
    "start": "cross-env NODE_ENV=production node ./build/main.js",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test:client": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:server": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "knex": "knex",
    "m:make": "knex migrate:make",
    "m:latest": "knex migrate:latest",
    "m:rollback": "knex migrate:rollback",
    "s:make": "knex seed:make",
    "s:run": "knex seed:run",
    "db": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "pdb": "cross-env NODE_ENV=production knex migrate:rollback && cross-env NODE_ENV=production knex migrate:latest && cross-env NODE_ENV=production knex seed:run"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.0",
    "@nuxtjs/apollo": "^4.0.1-rc.4",
    "@nuxtjs/eslint-config": "^4.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/moment": "^1.6.0",
    "@types/uuid": "^8.0.0",
    "@vue/test-utils": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.5.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.0",
    "csv-parser": "^2.3.3",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.3",
    "kakojs": "^0.1.60",
    "knex": "^0.21.11",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.27",
    "moment": "^2.24.0",
    "nuxt": "^2.11.0",
    "objection": "^2.2.0",
    "pg": "^8.3.0",
    "pino": "^6.6.0",
    "pluralize": "^8.0.0",
    "sass": "^1.28.0",
    "uniqid": "^5.2.0",
    "uuid": "^8.1.0",
    "vue": "^2.6.12",
    "vue-apollo": "^3.0.3",
    "vue-axios": "^3.1.3",
    "vue-upload-component": "^2.8.20",
    "xml-parser": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/parser": "^7.11.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-do-expressions": "^7.10.1",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.0.1",
    "babel-plugin-bulk-import": "^2.0.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-root-import": "^6.5.0",
    "backpack-core": "^0.8.4",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-vue": "^7.0.1",
    "jest": "^26.0.1",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "^10.0.1",
    "supertest": "^6.0.0",
